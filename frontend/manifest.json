{
  "name": "short_url_ext",
  "short_name": "short_url_ext",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "short_url_ext",
  "permissions": [
    "tabs",
    "webRequest",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["<all_urls>"]
    }
  ]
}